{
  "name": "@livestack/transcribe",
  "version": "0.0.24",
  "liencese": "MIT",
  "dependencies": {
    "@livestack/client": "^0.0.24",
    "@livestack/core": "^0.0.24",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^16",
    "@types/uuid": "^9.0.7",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "openai": "^4",
    "react": "^18",
    "uuid": "^9",
    "zod": "^3"
  },
  "scripts": {
    "build": "yarn build-client && yarn build-server",
    "build-client": "rimraf client && rollup -c rollup.config.mjs && cp src/client/raw-pcm-processor.js client/raw-pcm-processor.js",
    "build-server": "rimraf server && tsc --project tsconfig-server.json"
  },
  "exports": {
    "./client": {
      "default": "./client/index.js",
      "types": "./client/index.d.ts"
    },
    "./server": {
      "import": "./server/index.js",
      "require": "./server/index.js",
      "defualt": "./server/index.js",
      "types": "./server/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "client",
    "server",
    "package.json"
  ]
}
